<!DOCTYPE html>
<html>
<head>
  <title>Cabitica</title>
  <%= stylesheet_link_tag    'application', media: 'all', 
    'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if notice %>
    <div class="callout primary">
      <%= notice %>
    </div>
  <% end %>
  <% if alert %>
    <div class="callout alert">
      <%= alert %>
    </div>
  <% end %>

<div class="container">
  <%= yield %>
</div>

</body>
</html>
